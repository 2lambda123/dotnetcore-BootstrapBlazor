@namespace BootstrapBlazor.Components
@inherits BootstrapComponentBase

@if(anchorTarget is not null)
{
    <NavLink title="@Title" class="@ClassString" style="@StyleClassString" href="@To" target="@Target.ToString()" Match="@Match" @onclick="@ClickHandler">
        @ChildContent
    </NavLink>
}
else
{
    <Anchor title="@Title" Target="@anchorTarget" class="@ClassString" style="@StyleClassString" @onclick="@ClickHandler">
        @ChildContent
    </Anchor>        
}

